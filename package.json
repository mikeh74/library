{
  "name": "library",
  "version": "1.0.0",
  "description": "Django Example app",
  "browserslist": "> 0.5%, last 2 versions, not dead",
  "scripts": {
    "style-lint": "stylelint ./library/src/scss/**/*.scss",
    "style-lint-fix": "stylelint --fix ./library/src/scss/**/*.scss",
    "sass-compile": "sass ./library/src/scss/theme.scss ./library/static/css/theme.css",
    "sass-autoprefix": "postcss ./library/static/css/theme.css --use autoprefixer -o ./library/static/css/theme.css",
    "sass-minify": "postcss ./library/static/css/theme.css --use cssnano -o ./library/static/css/theme.min.css",
    "sass-build": "npm run sass-compile && npm run sass-autoprefix && npm run sass-minify",
    "sass-watch": "sass --watch ./library/src/scss/theme.scss ./library/static/css/theme.css",
    "javascript-linter": "eslint --fix ./static/*.js",
    "webpack-dev": "webpack --mode development",
    "webpack-prod": "webpack --mode production"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/mikeh74/library.git"
  },
  "author": "",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/mikeh74/library/issues"
  },
  "homepage": "https://github.com/mikeh74/library#readme",
  "devDependencies": {
    "@babel/core": "^7.20.2",
    "@babel/preset-env": "^7.20.2",
    "autoprefixer": "^10.4.18",
    "babel-loader": "^9.1.0",
    "cssnano": "^6.1.0",
    "eslint": "^7.30.0",
    "eslint-config-google": "^0.14.0",
    "postcss": "^8.4.35",
    "postcss-cli": "^11.0.0",
    "sass": "^1.56.1",
    "stylelint": "^16.0.2",
    "stylelint-checkstyle-formatter": "^0.1.2",
    "stylelint-config-standard": "^35.0.0",
    "stylelint-scss": "^6.0.0",
    "webpack": "^5.75.0",
    "webpack-cli": "^5.0.0"
  },
  "dependencies": {
    "bootstrap": "^5.2.2",
    "caniuse-lite": "^1.0.30001431"
  }
}
